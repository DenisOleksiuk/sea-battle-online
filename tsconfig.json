{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "module": "es6",
    "target": "es5",
    "strict": true,
    "jsx": "react",
    "allowJs": true
  },
  "include": ["public/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"],
  "extends": "@tsconfig/recommended/tsconfig.json",
  "display": "Recommended",
  "$schema": "https://json.schemastore.org/tsconfig"
}